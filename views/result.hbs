<div class="container">
    <div class="row">
        <div class="col-12">
            {{#each pages}}
                <div class="alert alert-success" role="alert">
                    <strong>ID:{{this.id}}</strong> - Name: {{this.pageName}} - Description: {{this.pageDescription}}  - URL: {{this.url}}
                </div>
            {{/each}}
        </div>
        <div class="pages">
            <p>
                <strong>Number of pages:{{this.numPages}}</strong>
            </p>
        </div>
        <div class="pagination">
            <input type="button" id="next" onclick="" value="Next 10 results">
            <a href="?page=1">1</a>
            <a href="?page=2">2</a>
            <input type="button" id="prev" onclick="" value="Previous 10 results">
        </div>
    </div>
</div>