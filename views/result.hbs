<div class="container">
    <div class="row">
        <div class="col-12">
            {{#each pages}}
                <div class="alert alert-success" role="alert">
                    <strong>ID:{{this.id}}</strong> - Name: {{this.pageName}} - Description: {{this.pageDescription}}  - URL: {{this.url}}
                </div>
            {{/each}}
        </div>
    </div>
</div>