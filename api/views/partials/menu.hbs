<div class="container">
    <div class="row">
        <h2>Categories:</h2>
        <div class="col-12">
            {{#each categories}}
                <div class="alert alert-success" role="alert">
                    <a href="/category?id={{this.id}} ">{{this.name}}</a>
                </div>
            {{/each}}
        </div>
    </div>
</div>